{
	"name": "ma-governance-api",
	"version": "1.14.1",
	"description": "Project of api in Ma Governance API",
	"main": "index.js",
	"scripts": {
		"start": "npx pm2-runtime start ./ecosystem.config.js",
		"start:dev": "npx tsx watch index.ts",
		"db:up": "docker-compose down && docker-compose up",
		"db:down": "docker-compose down",
		"stop": "npx pm2 kill",
		"build": "rm -rf ./dist && npx tsc",
		"test": "npx vitest --config vite.config.ts",
		"test:cov": "npx vitest --coverage --config vite.config.ts",
		"knex:migrate:make": "npx knex migrate:make --knexfile knexfile.ts -x ts",
		"knex:migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
		"knex:migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.ts",
		"add:patch": "npm version patch && git status && git add --all  &&  git commit -m  \" Add new Patch in Version\" && git push",
		"add:minor": "npm version minor && git status && git add --all  &&  git commit -m  \" Add new Minor in Version\" && git push",
		"add:major": "npm version major && git status && git add --all  &&  git commit -m  \" Add new Major in Version\" && git push",
		"up:tags": "git push --follow-tags",
		"deps": "npm i -g typescript knex",
		"dev": "npm run start:dev",
		"release:start": "VERSION=$(node -p \"require('./package').version\") && git flow release start $VERSION",
		"release:finish": "BRANCH=$(node -p \"require('./package').version\") && git flow release finish $BRANCH",
		"athon:c:s": "zsh bin/console/create-service.sh",
		"athon:c:m": "zsh bin/console/create-module.sh",
		"athon:c:md": "zsh bin/console/create-model.sh",
		"athon:c:t": "zsh bin/console/create-types.sh",
		"athon:c:e": "zsh bin/console/create-entity.sh",
		"athon:c:c": "zsh bin/console/create-config.sh",
		"deploy:prod": "git push heroku main",
		"deploy:dev": "git push heroku develop"
	},
	"keywords": [
		"API",
		"TypeScript",
		"NodeJS",
		"Postgres",
		"Redis"
	],
	"author": "RafaDraw",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+{{repository}}"
	},
	"homepage": "",
	"_moduleAliases": {
		"@root": "./",
		"@deep": "src/some/very/deep/directory/or/file",
		"@my_module": "lib/some-file.js",
		"something": "src/foo"
	},
	"dependencies": {
		"aws-sdk": "^2.1311.0",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.1",
		"cors": "^2.8.5",
		"dayjs": "^1.11.8",
		"express": "^4.18.2",
		"http": "^0.0.1-security",
		"http-status": "^1.6.2",
		"https": "^1.0.0",
		"is-empty": "^1.2.0",
		"jwt-simple": "^0.5.6",
		"knex": "^2.4.2",
		"md5": "^2.3.0",
		"module-alias": "^2.2.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"multer-s3": "^3.0.1",
		"mysql": "^2.18.1",
		"mysql2": "^3.1.2",
		"nodemailer": "^6.9.1",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.9.0",
		"pm2": "^5.2.2",

		"@types/bcrypt": "^5.0.0",
		"@types/body-parser": "^1.19.2",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/faker": "^6.6.9",
		"@types/is-empty": "^1.2.1",
		"@types/jwt-simple": "^0.5.33",
		"@types/md5": "^2.3.2",
